
{
    "manifest_version": 3,
    "name": "QuestLinker",
    "version": "1.0",
    "description": "Give the Links of Same type of Question in Different Platform",
    "permissions": [
      "storage",
      "activeTab",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "logo.png"
      }
    },
    "icons": {
      "128": "logo.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
      }
    ]
  }
  